{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "name": "cosmos",
      "type": "cosmosDB",
      "direction": "out",
      "ConnectionStringSetting": "CosmosDBConnection",
      "databaseName": "ratingsapi",
      "collectionName": "ratings",
      "createIfNotExists": true
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
